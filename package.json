{
  "name": "posifocus-spa",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^5.5.1",
    "jquery": "^3.4.1",
    "jquery-ui": "^1.12.1",
    "npm": "^6.4.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "react-simple-storage": "^1.2.2",
    "react-toggle": "^4.0.2",
    "react-transition-group": "^2.4.0"
  },
  "scripts": {
    "start": "npm run sass:build && react-scripts start",
    "build": "npm run sass:build && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "deploy": "npm run build && surge ./build 'https://posifocus.surge.sh'",
    "sass:watch": "node-sass -w src/styles/scss -o src/styles/css",
    "sass:build": "node-sass src/styles/scss -o src/styles/css --source-map true",
    "t:deploy": "npm run build && serve -s build"
  },
  "devDependencies": {
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "node-sass": "^4.9.3"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ]
}
